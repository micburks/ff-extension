{

  "manifest_version": 2,
  "name": "brooser",
  "version": "1.0",
  "description": "Does brooser stuff",
  "homepage_url": "https://github.com/micburks/ff-extension",
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "broose it",
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": ["background-script.js"]
  },
  "web_accessible_resources": [
    "pages/blocked.html"
  ]
}
