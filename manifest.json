{

  "manifest_version": 2,
  "name": "brooser",
  "version": "1.0",
  "description": "Does brooser stuff",
  "homepage_url": "https://github.com/micburks/ff-extension",
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "broose it",
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-scripts/blocked.js"]
    }
  ]

}
